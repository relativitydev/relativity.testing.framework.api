{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\Relativity.Testing.Framework.Api"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "cleanupCacheHistory": false,
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "fusion/**"
        ]
      }
    ],
    "dest": "_site",
    "disableGitFeatures": false,
    "fileMetadataFiles": [],
    "ForceRebuild": true,
    "globalMetadataFiles": [],
    "keepFileLink": false,
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "postProcessors": [],
    "template": [
      "default",
	  "templates/relativity"
    ],
    "xref": [
      "https://relativitydev.github.io/relativity.testing.framework/xrefmap.yml"
    ]
  }
}